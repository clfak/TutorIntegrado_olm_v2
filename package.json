{
  "name": "learner-model-gql-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "concurrently -m 1 -r pnpm:generate \"next build\" pnpm:pretty:check",
    "predev": "pnpm generate",
    "dev": "concurrently -r next pnpm:gql:watch pnpm:open:browser",
    "generate": "graphql-codegen --config codegen.yaml",
    "gql": "graphql-codegen --config codegen.yaml",
    "gql:watch": "graphql-codegen --config codegen.yaml --watch",
    "open:browser": "wait-on -s 1 http://localhost:3000 && open-cli http://localhost:3000",
    "pretty": "prettier --write \"**/*.{ts,tsx,json,js,jsx}\"",
    "pretty:check": "prettier --check \"**/*.{ts,tsx,json,js,jsx}\"",
    "start": "next start"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.1",
    "@chakra-ui/react": "^1.7.4",
    "@chakra-ui/system": "^1.9.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/core": "^2.4.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@matejmazur/react-katex": "^3.1.3",
    "better-react-mathjax": "^2.0.2",
    "framer-motion": "^6.5.1",
    "graphql": "^16.6.0",
    "immutability-helper": "^3.1.1",
    "katex": "0.16.4",
    "localforage": "^1.10.0",
    "mathjax-react": "1.0.6",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dnd": "14.0.5",
    "react-dnd-html5-backend": "14.1.0",
    "react-dnd-touch-backend": "14.1.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.8.0",
    "react-joyride": "2.5.3",
    "react-mathquill": "1.0.3",
    "react-notification-badge": "1.5.1",
    "react-query": "^3.39.3",
    "rq-gql": "^0.4.3",
    "serialize-error": "^9.1.1",
    "valtio": "1.2.9"
  },
  "devDependencies": {
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@types/node": "^16.18.16",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.6.0",
    "eslint": "8.36.0",
    "eslint-config-next": "12.0.8",
    "open-cli": "^7.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.1"
  },
  "engines": {
    "pnpm": ">=7.30.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core"
      ],
      "allowedVersions": {
        "graphql": "*",
        "framer-motion": "*"
      }
    }
  }
}
